{
    "manifest_version": 2,
    "name": "BBQ Cook Log",
    "version": "1",
    "permissions": [
        "https://api-content.dropbox.com/",
        "storage"],
    "app": {
        "background": {
            "scripts": ["bower_components/jquery/jquery.js",
                "bower_components/jquery-ui/ui/jquery-ui.js",
                "scripts/services/dropbox-datastores-0.1.0-b6.js",
                "bower_components/angular/angular.js",
                "bower_components/fullcalendar/fullcalendar.js",
                "bower_components/angular-resource/angular-resource.js",
                "bower_components/angular-cookies/angular-cookies.js",
                "bower_components/angular-sanitize/angular-sanitize.js",
                "bower_components/angular-route/angular-route.js",
                "bower_components/angular-bootstrap/ui-bootstrap-tpls.js",
                "bower_components/angular-ui-calendar/src/calendar.js",
                "bower_components/dropstore-ng/dropstore-ng.js",
                "scripts/app.js",
                "scripts/models/bbq-log.js",
                "scripts/services/data-service.js",
                "scripts/services/uuid-service.js",
                "scripts/controllers/main.js",
                "scripts/controllers/calendar.js",
                "scripts/controllers/cooklog.js",
                "scripts/controllers/cooktimer.js",
                "scripts/controllers/menu.js",
                "scripts/controllers/add-event.js",
                "scripts/controllers/add-cooking-procedure-log.js",
                "scripts/controllers/add-preparation-log.js",
                "scripts/controllers/add-weather-log.js",
                "scripts/controllers/add-cooker-log.js",
                "scripts/controllers/add-results-log.js",
                "background.js"]
            }
        },
    "minimum_chrome_version": "28",
    "content_security_policy": "script-src 'self' https://apis.google.com https://ssl.google-analytics.com https://platform.twitter.com https://chrome.google.com; object-src 'self'"
}